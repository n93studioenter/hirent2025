<script>
    $(document).ready(function () {
        loadStoreProducts(0)
    });
    function mdMoreStore() {
        $("#mdStore").modal("toggle");
    }
    function loadStoreProducts(id) {
        var param = {
            MainCateID:id
        }
        $.ajax({
            url: '/Productdetail/ProductShopList',
            dataType: 'html',
            data: param,
            type: "GET",
            async: false,
            success: function (data, status, xhr) {
                $("#storelistproduct").html(data);
            },
            error: function (xhr, status, error) {
                alert(status);
            }
        });
    }
    var arrayListsl = [];
    function slmore(dis) {
        $(dis).toggleClass('md_productshop_title3');
        //Update lại array
        arrayListsl = [];
        $('.md_productshop_title3').each(function () {
            arrayListsl.push($(this).attr("data-id"));
        });
        var getsplit = "";
        for (i = 0; i < arrayListsl.length; i++) {
            getsplit += arrayListsl[i] + ',';
        }

        var param = {
            id: getsplit
        }
        $.ajax({
            url: '/Productdetail/ProductAddmore',
            dataType: 'html',
            data: param,
            type: "GET",
            async: false,
            success: function (data, status, xhr) {
                $("#addmorepd").html(data);
            },
            error: function (xhr, status, error) {
                alert(status);
            }
        });
    }
</script>