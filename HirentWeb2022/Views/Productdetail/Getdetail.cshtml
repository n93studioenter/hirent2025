
<div id="breadcrumb">
    <div class="container">
        <ul class="breadcrumb">
            <li>
                <a href="#">
                    @if (ViewBag.lang == "vi")
                    {

                        <span>
                            Trang chủ
                        </span>
                    }
                    else
                    {
                        <span>
                            Home
                        </span>
                    }
                </a>
            </li>
            <li>
                @if (ViewBag.lang == "vi")
                {
                    @ViewBag.getmainname.MainCateName
                }
                else
                {
                    @ViewBag.getmainname.Language
                }

            </li>
            <li>
                @if (ViewBag.lang == "vi")
                {
                    @ViewBag.getmainname1.SubCate1Name
                }
                else
                {
                    @ViewBag.getmainname1.Language
                }
            </li>
            <li>
                @if (ViewBag.lang == "vi")
                {
                    @ViewBag.getmainname2.SubCate2Name
                }
                else
                {
                    @ViewBag.getmainname2.Language
                }
            </li>
            <li>
                @if (Model != null)
                {
                    foreach (var br in Model)
                    {
                        if (ViewBag.lang == "vi")
                        {
                            @br.ProductName
                        }
                        else
                        {
                            @ViewBag.Translate.ProductName_EN
                        }
                    }
                }
            </li>
        </ul>
    </div>
</div>

